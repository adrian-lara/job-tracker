<h4>New Contact</h4>
<%= form_for([@company, @contact]) do |f| %>
  <%= f.label :full_name, class: 'label' %>
  <%= f.text_field :full_name, class: 'text_field' %><br>

  <%= f.label :position, class: 'label' %>
  <%= f.text_field :position, class: 'text_field' %><br>

  <%= f.label :email, class: 'label' %>
  <%= f.text_field :email, class: 'text_field' %><br>

  <%= f.submit class: "submit_button" %>
<% end %>
